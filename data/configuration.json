{
	"patientsQueue": {
		"recallDuration": {
			"months": 1
		}
	},
	"storage": {
		"databaseDirectory": "data"
	},
	"server" : {
		"port": 8081,
		"host": "localhost",
		"url_server": "patients-queue.famille-besson.com",
		"ssl": {
			"enable": false,
			"certFile": "data/ssl/cert.crt",
			"keyFile": "data/ssl/key.key"
		}
	},
	"logs": {
		"file": "data/patients-queue.log",
		"max_size": 102400,
		"max_files": 5,
		"level": "debug"
	},
	"vuejs": {
		"debug": true,
		"pageSize": 5
	}
}
